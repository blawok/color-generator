{
    "dataset": "ColorsDataset",
    "model": "ColorModel",
    "network": "Distilbert",
    "device": "cpu",
    "dataset_args": {
        "path": "raw/colors.csv",
        "batch_size": 16
    },
    "network_args": {
        "freeze": true
    },
    "train_args": {
        "epochs": 25,
        "early_stopping": {
            "patience": 3,
            "verbose": true,
            "delta": 1e-3
        },
        "criterion": {
            "object": "torch.nn.MSELoss",
            "params": {
                "reduction": "mean"
            }
        },
        "optimizer": {
            "object": "torch.optim.AdamW",
            "params": {
                "lr": 3e-4,
                "weight_decay": 0.01
            }
        }
    }
}
