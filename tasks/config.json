{
    "dataset": "ColorsDataset",
    "model": "ColorModel",
    "network": {
        "name": "AnyBertNetwork",
        "args": {
            "architecture": "distilbert-base-uncased",
            "freeze": true
        }
    },
    "device": "cpu",
    "dataset_args": {
        "path": "raw/colors.csv",
        "batch_size": 16
    },
    "train_args": {
        "epochs": 25,
        "early_stopping": {
            "patience": 3,
            "verbose": true,
            "delta": 1e-3
        },
        "criterion": {
            "object": "torch.nn.MSELoss",
            "params": {
                "reduction": "mean"
            }
        },
        "optimizer": {
            "object": "torch.optim.AdamW",
            "params": {
                "lr": 3e-4,
                "weight_decay": 0.01
            }
        }
    }
}
